{
  "name": "shelfdat",
  "version": "0.1.0",
  "description": "ShelfDat is a powerful and user-friendly solution designed to help individuals manage their inventory efficiently. Keeping track of personal items, this app provides the tools you need to stay organized and informed.",
  "homepage": "https://shelfdat.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:codeengie/shelfdat.git"
  },
  "engines": {
    "npm": ">=6.14.8",
    "node": ">=14.15.0"
  },
  "private": true,
  "scripts": {
    "servedev": "vue-cli-service serve --mode dev",
    "serve": "vue-cli-service serve --mode prod",
    "serveprod": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "aws-amplify": "^4.3.35",
    "chart.js": "^3.9.1",
    "core-js": "^3.8.3",
    "ua-parser-js": "^1.0.32",
    "vue": "^3.2.13",
    "vue-chartjs": "^4.1.1",
    "vue-router": "^4.0.6",
    "vue3-carousel": "^0.2.9",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
